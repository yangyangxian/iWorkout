# Nuxt Environment Configuration for China Development

# Disable telemetry and analytics that might connect to blocked services
NUXT_TELEMETRY_DISABLED=1

# Disable automatic updates check
NUXT_CLI_TELEMETRY=0

# Speed up development by disabling some features
NODE_ENV=development

# Disable devtools if not needed (can speed up startup)
# NUXT_DEVTOOLS_ENABLED=false
